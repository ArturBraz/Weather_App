generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WeatherLog {
  id        Int      @id @default(autoincrement())
  city      String //cidade pesquisada
  lat       Float //coordenadas
  lon       Float //coordenadas
  response  Json
  createdAt DateTime @default(now())
}
